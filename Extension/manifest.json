{
  "name": "Notecaster",
  "version": "6.9",
  "description": "A real-time cloud computing extension",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "Notecaster",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
